{
  "name": "@orbkit/editor",
  "version": "0.0.1",
  "type": "module",
  "description": "Visual editor for orbkit scenes",
  "author": "Oryan Moshe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oryanmoshe/orbkit.git",
    "directory": "packages/editor"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./src/styles.css"
  },
  "files": ["dist", "src/styles.css"],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --emitDeclarationOnly --outDir dist/types",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "orbkit": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "orbkit": "workspace:*",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  }
}
